@page
@model WebApplication.Pages.Areas.Designers.Artwork.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].Image)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].UpdatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].DeletedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtWork[0].ObjectId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ArtWork) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Image)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeletedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObjectId)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
